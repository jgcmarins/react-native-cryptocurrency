  "bugs": {
    "url": "https://github.com/jgcmarins/react-native-cryptocurrency/issues"
  },
  "homepage": "https://github.com/jgcmarins/react-native-cryptocurrency#readme",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "mdx-code": "^1.1.3",
    "mdx-deck": "^1.8.2",
    "mdx-deck-code-surfer": "^0.5.5",
    "mdx-deck-live-code": "^1.0.0",
    "raw-loader": "^1.0.0"
