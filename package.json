{
  "name": "react-native-cryptocurrency",
  "version": "1.0.0",
  "description": "React Native app for Cryptocurrency",
  "main": "index.js",
  "scripts": {
    "build": "mdx-deck build src/deck.mdx",
    "help": "mdx-deck",
    "image": "mdx-deck screenshot src/deck.mdx",
